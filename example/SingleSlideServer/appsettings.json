{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Image": {
    "Path": "image.tiff",
    "TileCachePath": "./tile_cache",
    "EnableDiskCache": true,
    "TileFormat": {
      "Default": "jpeg",
      "LosslessFormat": "png",
      "LosslessLevelCount": 1,
      "JpegQuality": 90
    }
  }
}
